{
  "name": "@pagepocket/cli",
  "version": "0.4.2",
  "description": "CLI for capturing offline snapshots of web pages.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "tsx --test specs/**/*.test.ts"
  },
  "bin": {
    "pp": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.25.0",
  "dependencies": {
    "@oclif/core": "^4.0.9",
    "@pagepocket/lib": "workspace:*",
    "@pagepocket/lighterceptor": "workspace:*",
    "@pagepocket/lighterceptor-adapter": "workspace:*",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.12",
    "got": "^11.8.6",
    "ora": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "tsx": "^4.19.3",
    "typescript": "^5.4.5"
  },
  "oclif": {
    "bin": "pp",
    "commands": {
      "strategy": "single",
      "target": "dist/cli.js"
    }
  }
